from itertools import count

id_part = count(1)
participacoes = []

id_usuario = int(input("ID usuário: "))
id_evento = int(input("ID evento: "))
presenca = input("Presença (sim/não): ")

participacoes.append({
    "id": next(id_part),
    "id_usuario": id_usuario,
    "id_evento": id_evento,
    "presenca": presenca
})
print(participacoes)
