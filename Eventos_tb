from itertools import count

id_evento = count(1)
eventos = []

nome = input("Nome do evento: ")
descricao = input("Descrição: ")
data = input("Data (AAAA-MM-DD): ")
local = input("Local: ")
responsavel = input("Responsável: ")

eventos.append({
    "id": next(id_evento),
    "nome": nome,
    "descricao": descricao,
    "data": data,
    "local": local,
    "responsavel": responsavel
})
print(eventos)
